{
  "$schema": "vscode://schemas/color-theme",
  "name": "Alabaster",
  "tokenColors": [
    {
      "name": "Comments",
      "scope": ["comment", "punctuation.definition.comment"],
      "settings": {
        "foreground": "#808080",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Strings",
      "scope": [
        "string",
        "string.regexp",
        "constant.other.symbol",
        "punctuation.definition.string",
        "string.quoted.double.go",
        "entity.name.import.go"
      ],
      "settings": {
        "foreground": "#008000"
      }
    },
    {
      "name": "Strings: Escape Sequences",
      "scope": "constant.character.escape",
      "settings": {
        "foreground": "#777777"
      }
    },
    {
      "name": "Invalid",
      "scope": "invalid",
      "settings": {
        "background": "#96000014",
        "foreground": "#660000"
      }
    },
    {
      "name": "Extra: Diff Range",
      "scope": ["meta.diff.range", "meta.diff.index", "meta.separator"],
      "settings": {
        "background": "#DDDDFF",
        "foreground": "#434343"
      }
    },
    {
      "name": "Extra: Diff From",
      "scope": "meta.diff.header.from-file",
      "settings": {
        "background": "#FFDDDD",
        "foreground": "#434343"
      }
    },
    {
      "name": "Extra: Diff To",
      "scope": "meta.diff.header.to-file",
      "settings": {
        "background": "#DDFFDD",
        "foreground": "#434343"
      }
    },
    {
      "name": "Go Keyword",
      "scope": [
        "keyword.control.go",
        "keyword.function.go",
        "keyword.import.go",
        "keyword.package.go",
        "keyword.type.go",
        "keyword.var.go",
        "keyword.const.go",
        "keyword.struct.go",
        "keyword.interface.go",
        "keyword.map.go",
        "keyword.chan.go",
        "keyword.fallthrough.go",
        "keyword.select.go",
        "keyword.defer.go",
        "keyword.go.go",
        "keyword.goto.go",
        "keyword.control.import.go",
        "keyword.channel.go",
        "keyword.string.go",
        "storage.type.string.go",
        "storage.type.byte.go",
        "storage.type.error.go",
        "storage.type.numeric.go",
        "storage.type",
        "storage.modifier"
      ],
      "settings": {
        "foreground": "#0000F0"
      }
    },
    {
      "name": "builtin constant",
      "scope": ["constant.language"],
      "settings": {
        "foreground": "#0000dc"
      }
    },
    {
      "name": "Go variable constant",
      "scope": ["variable.other.constant.go"],
      "settings": {
        "foreground": "#660E7A",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Go Operator",
      "scope": [
        "keyword.operator",
        "keyword.operator.address.go",
        "keyword.operator.arithmetic.go",
        "keyword.operator.assignment.go",
        "keyword.operator.bitwise.go",
        "keyword.operator.channel.go",
        "keyword.operator.comparison.go",
        "keyword.operator.ellipsis.go",
        "keyword.operator.increment-decrement.go",
        "keyword.operator.logical.go",
        "keyword.operator.pointer.go",
        "keyword.operator.special.go",
        "punctuation.definition.begin.bracket.square.go",
        "punctuation.definition.end.bracket.square.go"
      ],
      "settings": {
        "foreground": "#000000",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Go Punctuation (Brackets, Comma, Dot, Semicolon, Colon)",
      "scope": [
        "punctuation.section.parenthesis.begin.go",
        "punctuation.section.parenthesis.end.go",
        "punctuation.section.block.begin.go",
        "punctuation.section.block.end.go",
        "punctuation.definition.begin.bracket.round.go",
        "punctuation.definition.end.bracket.round.go",
        "punctuation.definition.begin.bracket.curly.go",
        "punctuation.definition.end.bracket.curly.go"
      ],
      "settings": {
        "foreground": "#000000"
      }
    },
    {
      "name": "Go Punctuation ( Comma, Dot, Semicolon, Colon)",
      "scope": [
        "punctuation.section.brackets.begin.go",
        "punctuation.section.brackets.end.go",
        "punctuation.separator.comma.go",
        "punctuation.accessor.dot.go",
        "punctuation.terminator.semicolon.go",
        "punctuation.separator.colon.go"
      ],
      "settings": {
        "foreground": "#000000",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Go Builtin Constant",
      "scope": [
        "constant.language.go",
        "entity.name.function.support.builtin.go"
      ],
      "settings": {
        "foreground": "#0000F0"
      }
    },
    {
      "name": "Go Placeholder",
      "scope": ["constant.other.placeholder.go"],
      "settings": {
        "foreground": "#000080",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Go Builtin Function Call",
      "scope": ["support.function.builtin.go"],
      "settings": {
        "foreground": "#C8C8C8",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Go Builtin Variable",
      "scope": ["support.variable.builtin.go"],
      "settings": {
        "foreground": "#0000F0",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Go Exported Function Declaration",
      "scope": ["entity.name.function.go"],
      "settings": {
        "foreground": "#A00000",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Go Function Call",
      "scope": ["meta.function-call.go entity.name.function.go"],
      "settings": {
        "foreground": "#9D2D2E",
        "fontStyle": "bold"
      }
    },

    {
      "name": "Go Function Parameter",
      "scope": ["variable.parameter.go"],
      "settings": {
        "foreground": "#323232",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Go Label",
      "scope": ["entity.name.label.go"],
      "settings": {
        "fontStyle": "bold"
      }
    },
    {
      "name": "Go Local Variable",
      "scope": ["variable.other.local.go", "variable.other.assignment.go"],
      "settings": {
        "foreground": "#000000",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Go Method Receiver",
      "scope": ["variable.parameter.receiver.go"],
      "settings": {
        "foreground": "#9C9C9C"
      }
    },
    {
      "name": "Go Number",
      "scope": ["constant.numeric.go"],
      "settings": {
        "foreground": "#0000F0",
        "fontStyle": "bold italic"
      }
    },
    {
      "name": "Go Package Name",
      "scope": ["entity.name.package.go", "entity.name.type.package.go"],
      "settings": {
        "foreground": "#000000",
        "fontStyle": "bold italic"
      }
    },
    {
      "name": "Go Package Import Path",
      "scope": ["string.quoted.double.import.go"],
      "settings": {
        "foreground": "#000000",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Go Type Reference",
      "scope": ["entity.name.type.go"],
      "settings": {
        "foreground": "#47AAAA",
        "fontStyle": "bold"
      }
    },

    {
      "name": "Go Struct/Package Variable/Member",
      "scope": [
        "variable.other.member.go",
        "entity.name.field.go",
        "variable.other.package.go",
        "variable.other.property.go"
      ],
      "settings": {
        "foreground": "#0A0064",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Go Variable/Member Call/Usage",
      "scope": [
        "variable.other.readwrite.go",
        "variable.other.object.property.go"
      ],
      "settings": {
        "foreground": "#BC0000"
      }
    },

    {
      "name": "Keywords (General)",
      "scope": ["keyword"],
      "settings": {
        "foreground": "#0000f0"
      }
    },
    {
      "name": "Numbers (General)",
      "scope": ["constant.numeric"],
      "settings": {
        "foreground": "#0000F0",
        "fontStyle": "bold italic"
      }
    },
    {
      "name": "Types (General)",
      "scope": ["entity.name.type", "support.type"],
      "settings": {
        "foreground": "#47AAAA",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Functions (General)",
      "scope": ["entity.name.function", "support.function"],
      "settings": {
        "foreground": "#A00000"
      }
    },
    {
      "name": "C++ Builtin Constant",
      "scope": [
        "constant.language.nullptr.cpp",
        "variable.language.this.cpp",
        "entity.name.function.support.builtin.cpp",
        "constant.language.true.cpp",
        "constant.language.false.cpp",
        "constant.language.NULL.cpp"
      ],
      "settings": {
        "foreground": "#C000C0"
      }
    },
    {
      "name": "C++ Builtin keyword",
      "scope": [
        "variable.other.unknown.protected.cpp",
        "variable.other.unknown.public.cpp",
        "variable.other.unknown.private.cpp"
      ],
      "settings": {
        "foreground": "#0000F0"
      }
    },
    {
      "name": "C++ Macro",
      "scope": ["entity.name.function.preprocessor.cpp"],
      "settings": {
        "foreground": "#1F542E",
        "fontStyle": "bold"
      }
    },
    {
      "name": "C++ Qt Macro",
      "scope": [
        "variable.other.unknown.Q_OBJECT.cpp",
        "variable.other.unknown.slots.cpp"
      ],
      "settings": {
        "foreground": "#1F542E",
        "fontStyle": "bold"
      }
    },
    {
      "name": "C++ enum",
      "scope": ["variable.other.enummember.cpp"],
      "settings": {
        "foreground": "#660E7A",
        "fontStyle": "bold"
      }
    },
    {
      "name": "html tag",
      "scope": ["entity.name.tag.html"],
      "settings": {
        "foreground": "#2F6F9F",
        "fontStyle": "bold"
      }
    },
    {
      "name": "vue tag",
      "scope": [
        "entity.name.tag.script.html.vue",
        "entity.name.tag.style.html.vue",
        "entity.name.tag.template.html.vue"
      ],
      "settings": {
        "foreground": "#800000",
        "fontStyle": "bold"
      }
    },
    {
      "name": "HTML declaration",
      "scope": [
        "text.html.basic meta.tag.sgml.html punctuation.definition.tag.html",
        "text.html.basic meta.tag.sgml.html meta.tag.sgml.doctype.html",
        "text.html.basic meta.tag.sgml.html meta.tag.sgml.doctype.html string.quoted.double.doctype.identifiers-and-DTDs.html"
      ],
      "settings": {
        "foreground": "#535353"
      }
    },
    {
      "name": "HTML character",
      "scope": [
        "text.html.basic constant.character.entity.html",
        "text.xml constant.character.entity.xml"
      ],
      "settings": {
        "foreground": "#e88501"
      }
    },
    {
      "name": "Tag name",
      "scope": "entity.name.tag",
      "settings": {
        "foreground": "#386ac3"
      }
    },
    {
      "name": "Tag start/end",
      "scope": [
        "punctuation.definition.tag.begin",
        "punctuation.definition.tag.end",
        "punctuation.definition.tag.html"
      ],
      "settings": {
        "foreground": "#386ac3"
      }
    },
    {
      "name": "Tag attribute",
      "scope": "entity.other.attribute-name",
      "settings": {
        "foreground": "#6d8600"
      }
    },
    {
      "name": "CSS selector",
      "scope": [
        "meta.selector.css entity.name.tag.css",
        "meta.selector.css entity.other.attribute-name.class.css",
        "meta.selector.css entity.other.attribute-name.id.css",
        "meta.selector.css meta.attribute-selector.css entity.other.attribute-name.attribute.css",
        "meta.selector.css meta.attribute-selector.css string.unquoted.attribute-value.css"
      ],
      "settings": {
        "foreground": "#386ac3"
      }
    },
    {
      "name": "CSS selector punctuation",
      "scope": "meta.selector.css meta.attribute-selector.css punctuation.separator.operator.css",
      "settings": {
        "foreground": "#535353"
      }
    },
    {
      "name": "CSS selector status",
      "scope": [
        "meta.selector.css entity.other.attribute-name.pseudo-class.css",
        "meta.selector.css entity.other.attribute-name.pseudo-element.css"
      ],
      "settings": {
        "foreground": "#535353"
      }
    },
    {
      "name": "CSS at-rule",
      "scope": [
        "keyword.control.at-rule",
        "meta.at-rule support.type.property-name"
      ],
      "settings": {
        "foreground": "#8431c5"
      }
    },
    {
      "name": "CSS at-rule support constant",
      "scope": "meta.at-rule support.constant",
      "settings": {
        "foreground": "#6d8600"
      }
    },
    {
      "name": "CSS at-rule operator",
      "scope": "meta.at-rule keyword.operator",
      "settings": {
        "foreground": "#386ac3"
      }
    },
    {
      "name": "CSS property-name keyword",
      "scope": "meta.property-name.css support.type.property-name.css",
      "settings": {
        "foreground": "#8431c5"
      }
    },
    {
      "name": "CSS property-value",
      "scope": "meta.property-value.css",
      "settings": {
        "foreground": "#535353"
      }
    },
    {
      "name": "CSS property-value keyword",
      "scope": [
        "meta.property-value.css support.constant",
        "meta.property-value.css support.function",
        "meta.property-value.css variable.parameter.misc.css",
        "meta.property-value.css constant.other.color.rgb-value.css"
      ],
      "settings": {
        "fontStyle": "",
        "foreground": "#e88501"
      }
    },
    {
      "name": "CSS property-value number",
      "scope": [
        "meta.property-value.css constant.numeric.css",
        "meta.property-value.css constant.numeric.css keyword.other.unit.css",
        "meta.at-rule constant.numeric.css keyword.other.unit.css"
      ],
      "settings": {
        "foreground": "#6d8600"
      }
    },
    {
      "name": "CSS color name",
      "scope": "meta.property-value.css support.constant.color.w3c-standard-color-name.css",
      "settings": {
        "foreground": "#386ac3"
      }
    },
    {
      "name": "CSS punctuation",
      "scope": [
        "punctuation.separator.key-value.css",
        "punctuation.terminator.rule.css",
        "punctuation.section.property-list.css"
      ],
      "settings": {
        "foreground": "#535353"
      }
    },
    {
      "name": "JS keywords 1",
      "scope": [
        "variable.language.js",
        "storage.type.js",
        "storage.type.function.js",
        "meta.function.json.js storage.type.function.js",
        "meta.class.instance.constructor",
        "keyword.operator.new keyword.operator.new.js"
      ],
      "settings": {
        "foreground": "#386ac3"
      }
    },
    {
      "name": "JS / TS keywords 2",
      "scope": [
        "support.function.js",
        "support.function.ts",
        "support.function.dom.js",
        "support.constant.js",
        "entity.name.function.js",
        "entity.name.function.ts",
        "meta.function.json.js variable.parameter.function.js"
      ],
      "settings": {
        "foreground": "#9D2D2E"
      }
    },
    {
      "name": "XML declaration",
      "scope": [
        "text.xml meta.tag.preprocessor.xml punctuation.definition.tag.begin.xml",
        "text.xml meta.tag.preprocessor.xml entity.name.tag.xml",
        "text.xml meta.tag.preprocessor.xml entity.other.attribute-name.xml",
        "text.xml meta.tag.preprocessor.xml string.quoted.double.xml",
        "text.xml meta.tag.sgml.doctype.xml punctuation.definition.tag.begin.xml",
        "text.xml meta.tag.sgml.doctype.xml keyword.doctype.xml"
      ],
      "settings": {
        "foreground": "#535353"
      }
    }
  ],
  "colors": {
    "editor.background": "#F7F7F7",
    "editor.foreground": "#000",
    "editor.lineHighlightBackground": "#F0F0F0",
    "editor.selectionBackground": "#BFDBFE",
    "editor.selectionHighlightBackground": "#CFCFCF81",
    "panel.background": "#F0F0F0",
    "sideBar.background": "#F0F0F0",
    "editorGroupHeader.tabsBackground": "#F0F0F0",
    "activityBar.background": "#F0F0F0",
    "activityBar.foreground": "#007ACC",
    "editorLineNumber.foreground": "#9DA39A",
    "editorCursor.foreground": "#007ACC",
    "editor.findMatchBackground": "#FFBC5D",
    "editor.findMatchHighlightBackground": "#FFD86381",
    "statusBar.background": "#DDDDDD",
    "statusBar.foreground": "#474747",
    "statusBar.debuggingBackground": "#FFBC5D",
    "statusBar.debuggingForeground": "#000",
    "statusBar.noFolderBackground": "#7A3E9D",
    "statusBar.noFolderForeground": "#fff",

    "list.activeSelectionBackground": "#DDDDDD",
    "list.activeSelectionForeground": "#000000",
    "list.inactiveSelectionBackground": "#E6E6E6",
    "list.focusHighlightForeground": "#007ACC",
    "focusBorder": "#CCCCCC",

    "quickInputList.focusForeground": "#000000",
    "quickInputList.focusBackground": "#DDDDDD",

    "editorSuggestWidget.selectedBackground": "#DDDDDD",
    "editorSuggestWidget.focusHighlightForeground": "#000000",
    "editorSuggestWidget.highlightForeground": "#000000",
    "editorSuggestWidget.selectedForeground": "#000000",

    "terminal.ansiWhite": "#BBBBBB",
    "terminal.ansiBlack": "#000000",
    "terminal.ansiBlue": "#325CC0",
    "terminal.ansiCyan": "#0083B2",
    "terminal.ansiGreen": "#448C27",
    "terminal.ansiMagenta": "#7A3E9D",
    "terminal.ansiRed": "#AA3731",
    "terminal.ansiYellow": "#CB9000",
    "terminal.ansiBrightWhite": "#FFFFFF",
    "terminal.ansiBrightBlack": "#777777",
    "terminal.ansiBrightBlue": "#007ACC",
    "terminal.ansiBrightCyan": "#00AACB",
    "terminal.ansiBrightGreen": "#60CB00",
    "terminal.ansiBrightMagenta": "#E64CE6",
    "terminal.ansiBrightRed": "#F05050",
    "terminal.ansiBrightYellow": "#FFBC5D"
  }
}
